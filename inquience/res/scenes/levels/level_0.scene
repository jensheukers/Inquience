	SetCameraPosition(0.000000,0.000000)
	AddKVP("BeginGameplay","True")
	AddKVP("x","300")
	AddKVP("y","200")
	BeginEntity()
	SetPosition(224.000000,544.000000)
	SetScale(512.000000,32.000000)
	SetTag("Entity")
		BeginComponent("Sprite")
			SetProperty("Texture","res/textures/placeholder.tga")
			SetProperty("SetUV",0.000000,0.000000,1.000000,0.000000,0.000000,1.000000,1.000000,1.000000)
		EndComponent()
		BeginComponent("BoxCollider")
			SetProperty("Outer",512.000000,32.000000)
			SetProperty("ScaleToOwner",205)
			SetProperty("DrawDebug",0)
		EndComponent()
	EndEntity()
end
